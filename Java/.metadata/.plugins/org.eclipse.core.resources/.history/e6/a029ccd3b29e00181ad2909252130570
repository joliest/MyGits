import java.util.*;
public class Test {
	public static void main(String[] args) {
		ArrayList<String> samples = new ArrayList<String>();
		
		samples.add("Joliver");
		samples.add("Khaye");
		samples.add("Arlene");
		samples.add("Noel");
		samples.add("Elaine");
		samples.add("Trixie");
		
		System.out.println(samples);
		
		for(int i = 0; i < samples.size(); i++) {
			System.out.println((i+1) + ". " + samples.get(i));
		}
	}
}
