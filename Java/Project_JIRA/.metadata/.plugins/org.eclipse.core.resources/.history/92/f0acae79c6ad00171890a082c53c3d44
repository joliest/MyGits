package com.tracker.Command;

import com.tracker.DatabaseHelper.DatabaseManager;

public class AttributeCommands implements Command {
	
	private DatabaseManager manager;
	
	public AttributeCommands(DatabaseManager manager) {
		this.manager = manager;
	}

	@Override
	public void add() {
		manager.add();
	}

	@Override
	public void update() {
		manager.update(idNumber, columnIndex, value);
	}

	@Override
	public void remove() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void view() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void undo() {
		// TODO Auto-generated method stub
		
	}

}
