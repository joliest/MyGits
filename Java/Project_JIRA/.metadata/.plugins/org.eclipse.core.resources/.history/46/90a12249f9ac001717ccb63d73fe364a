package com.tracker.Attribute.model;

import javax.servlet.http.HttpServletRequest;

import com.tracker.Module;

public class Attribute implements Module{
	
	private String name;
	private String dataType;
	
	private int id;
	private int groupId;
	private int length;
	
	private HttpServletRequest request;
	
	private DatabaseCommand addCommand;
	private DatabaseCommand modifyCommand;
	private DatabaseCommand removeCommand;
	
	public Attribute(HttpServletRequest request) {
		setHttpRequest(request);
	}
	
	public void setHttpRequest(HttpServletRequest request) {
		this.request = request;
	}

	public int createId() {
		
		return 0;
	}

	@Override
	public void add() {
		addCommand = new AddAttributeCommand(this);
		addCommand.execute();
	}

	@Override
	public void update() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void remove() {
		// TODO Auto-generated method stub
		
	}
	
	
}
